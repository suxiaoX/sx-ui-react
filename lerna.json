{
  "version": "independent",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "command": {
    "ignoreChange": [
      ".gitignore",
      ".log",
      ".md"
    ],
    "publish": "https://registry.npmjs.org/"
  },
  "changelog": {},
  "ignoreChanges": [
    "**/*.md",
    "**/*.test.ts",
    "**/*.e2e.ts",
    "**/demos/**",
    "**/fixtures/**",
    "**/dist/**",
    "**/lib/**",
    "**/es/**",
    "**/test/**"
  ]
}
